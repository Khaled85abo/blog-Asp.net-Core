@model IEnumerable<Post>

    @{}

<h2>Admin Panel</h2>
<a class="action-link" asp-controller="Panel" asp-action="Edit">Add A new Post</a>

<div>
    <ul>
        @foreach(var post in Model)
        {
            <li>
               @post.Title
                <a class="action-link" asp-controller="Panel" asp-action="Edit" asp-route-id="@post.Id">Edit</a>
               <a class="action-link" asp-controller="Panel" asp-action="Remove" asp-route-id="@post.Id">Delete</a>

            </li>
        }
    </ul>
</div>
